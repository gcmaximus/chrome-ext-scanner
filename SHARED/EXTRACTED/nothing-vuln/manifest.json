{
  "manifest_version": 3,
  "name": "No Vulnerabilities Extension",
  "version": "4.2",
  "description": "A simple extension that accepts user input",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",    
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
