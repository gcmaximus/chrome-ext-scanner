{
    "manifest_version": 3,
    "name": "Remove Youtube shorts",
    "version": "1.0",
    "description": "Removes youtube short video",
    "icons": {
        "48": "icon48.png"
    },
    "author": "dynamic-team",
    "permissions": [
        "storage",
        "webNavigation",
        "tabs",
        "scripting",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://example.com/",
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}