{
  "manifest_version": 3,
  "name": "H1 Replacer Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "debugger",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ]
}